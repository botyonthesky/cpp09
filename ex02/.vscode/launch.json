{
	"configurations": [
	{
		"name": "(gdb) PmergeMe",
		"type": "cppdbg", 
		"request": "launch",
		"program": "${workspaceRoot}/PmergeMe",
		"args": [
			"12",
			"2",
			"-1",

		],
		"stopAtEntry": false,
		"cwd": "${workspaceRoot}",
		"environment": [],
		"externalConsole": false,
		"MIMode": "gdb",
		"setupCommands": [
			{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": true
			},
			{
				"description": "Set Disassembly Flavor to Intel",
				"text": "-gdb-set disassembly-flavor intel",
				"ignoreFailures": true
			}
		],
		"preLaunchTask": "PmergeMe"
	}
	]
}


